{
    "name": "silverstripe/intercom",
    "description": "SilverStripe integration with Intercom.io",
    "type": "silverstripe-vendormodule",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/framework": "^4",
        "sminnee/tagmanager": "*",
        "intercom/intercom-php": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sam Minn√©e",
            "email": "sam@silverstripe.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SilverStripe\\Intercom\\": "code/",
            "SilverStripe\\Intercom\\Tests\\": "tests/"
        }
    },
    "replace": {
        "sminnee/silverstripe-intercom": "*"
    }
}
